{
  "agent": {
    "metrics_collection_interval": 10,
    "run_as_user": "root",
    "debug": true,
    "logfile": ""
  },
  "metrics": {
    "namespace": "MetricValueBenchmarkTest",
    "aggregation_dimensions": [
      [
        "host"
      ]
    ],
    "metrics_collected": {
      "disk": {
        "resources": [
          "*"
        ],
        "measurement": [
          "free",
          "inodes_free",
          "inodes_total",
          "inodes_used",
          "total",
          "used",
          "used_percent"
        ],
        "drop_device": true
      }
    },
    "force_flush_interval": 5
  }
}